<views:RBaseWindow x:TypeArguments="viewModels:MainViewModel" x:Class="FileRenameManager.Wpf.Views.MainWindow"
                   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                   xmlns:local="clr-namespace:FileRenameManager.Wpf"
                   xmlns:viewModels="clr-namespace:FileRenameManager.Wpf.ViewModels"
                   xmlns:views="clr-namespace:FileRenameManager.Wpf.Views"
                   mc:Ignorable="d"
                   Title="File Rename Manager" Background="WhiteSmoke" ResizeMode="CanMinimize" Height="400" Width="600" WindowState="Normal" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>

        </Grid.RowDefinitions>
        <TabControl Background="WhiteSmoke">
            <TabItem Header="Calendar Rename" Background="WhiteSmoke">
                <views:RenameCalenderUc x:Name="RenameCalenderUc"/>
            </TabItem>
        </TabControl>
        <StackPanel Grid.Row="1">
          
          <GroupBox Margin="3" Header="Log:">
              <TextBox  Height="150" Margin="5"></TextBox>
          </GroupBox>
        </StackPanel>
    </Grid>
</views:RBaseWindow>
